# Environment Variables for Swap
# Copy this file to .env and set your values

# Google Service Account (legacy support)
SERVICE_ACCOUNT_FILE="/path/to/your/service-account.json"

# Application settings (with SWAP_ prefix)
SWAP_DEBUG=false
SWAP_LOG_LEVEL=INFO

# Database settings
SWAP_DATABASE__URL="sqlite:///swap.db"
SWAP_DATABASE__ECHO=false

# Google API settings
SWAP_GOOGLE__SERVICE_ACCOUNT_FILE="/path/to/your/service-account.json"
SWAP_GOOGLE__SPREADSHEET_ID="1KKS89Y3M9xW6lI00qXAO45zyi7Xk5Y4DBGKqSDkfOZQ"
SWAP_GOOGLE__RANGE_NAME="Sheet1!A:M"
SWAP_GOOGLE__TIMEZONE="Europe/Dublin"
